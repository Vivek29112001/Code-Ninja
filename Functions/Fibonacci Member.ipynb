    "def checkMember(n):\n",
    "#Implement Your Code Here\n",
    "    a,b=0,1\n",
    "    temp=0\n",
    "    d=0\n",
    "    while temp<n:\n",
    "        temp=a+b\n",
    "        a=b\n",
    "        b=temp\n",
    "        d=temp\n",
    "    return d==n\n",
    "\n",
    "n=int(input())\n",
    "if(checkMember(n)):\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
