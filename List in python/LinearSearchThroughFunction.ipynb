    "def linear_search(li,ele):\n",
    "    #lie is the list and ele is the element to search\n",
    "    for i in range(len(li)):\n",
    "        if li[i]==ele:\n",
    "            return i\n",
    "    return -1\n",
    "li=[1,2,3,4,5]\n",
    "index= linear_search(li,5)\n",
    "print(index)"
   ]
