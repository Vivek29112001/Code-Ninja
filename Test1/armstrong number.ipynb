    "n=input()\n",
    "l=list(n)\n",
    "n=int(n)\n",
    "x=len(l)\n",
    "s=0\n",
    "for i in l:\n",
    "    s+=int(i)**x\n",
    "if s==n:\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
